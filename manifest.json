{
  "manifest_version": 2,
  "version": "1.1",
  "name": "Buscador DILSE",
  "description": "Añade un motor de búsqueda que permite buscar palabras directamente en el Diccionario de la Lengua de Signos Española (DILSE)",
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "DILSE",
      "search_url": "https://fundacioncnse-dilse.org/?buscar={searchTerms}",
      "favicon_url": "https://fundacioncnse-dilse.org/img/dilse.ico",
      "keyword": "@dilse"
      "is_default": false,
      "encoding": "UTF-8"
    }
  },
    "browser_specific_settings": {
    "gecko": {
      "id": "{ID GOES HERE}"
    }
  }
}
